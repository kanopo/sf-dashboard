{
  "name": "sf-next-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --watch ./src/i18n/config.js --watch ./src/i18n/locales --watch ./public/assets/icons --watch ./public/assets/images --ext ts,tsx,js,svg,json,jpeg,png --exec 'npm run images:convert && npm run i18n:type && npm run icons:type && NODE_ENV=development next dev'",
    "buildold": "npm run images:convert && npm run icons:type && npm run i18n:type && NODE_ENV=production next build",
    "build": "npm run icons:type && npm run i18n:type && NODE_ENV=production next build",
    "start": "NODE_ENV=production next start",
    "lint": "next lint",
    "git-hooks:init": "sh ./scripts/git-hooks:init.sh",
    "icons:type": "node ./scripts/icons:type.js",
    "images:convert": "sh ./scripts/images:convert.sh",
    "sdk:compile": "node ./scripts/sdk:compile.js",
    "translations:sort": "node ./scripts/translations:sort.js",
    "test:e2e": "playwright install && DEBUG=pw:webserver playwright test",
    "test:e2e:update-snapshots": "playwright install && DEBUG=pw:webserver playwright test --update-snapshots",
    "playwright:codegen": "playwright codegen localhost:3000",
    "playwright:show-report": "playwright show-report",
    "i18n:type": "node ./scripts/i18n:type.js"
  },
  "dependencies": {
    "@octokit/rest": "^19.0.11",
    "accept-language": "^3.0.18",
    "classnames": "^2.3.1",
    "encoding": "^0.1.13",
    "i18next": "^22.4.11",
    "i18next-resources-to-backend": "^1.1.2",
    "next": "^13.2.4",
    "next-compose-plugins": "^2.2.1",
    "next-i18next": "^13.2.2",
    "octokit": "^2.0.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "react-inlinesvg": "^3.0.0",
    "react-toastify": "^9.0.5",
    "server-only": "^0.0.1",
    "zustand": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/eslint-parser": "^7.18.2",
    "@next/bundle-analyzer": "^12.1.6",
    "@playwright/test": "1.31.2",
    "@soluzioni-futura/eslint-config-soluzioni-futura": "^2.2.0",
    "@types/google.accounts": "^0.0.6",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "autoprefixer": "^10.4.7",
    "eslint": "8.18.0",
    "eslint-config-next": "^13.2.4",
    "nodemon": "^2.0.21",
    "openapi-typescript-codegen": "^0.23.0",
    "postcss": "^8.4.14",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^14.1.0",
    "postcss-preset-env": "^7.7.2",
    "tailwindcss": "^3.1.4",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": "^18.14.0",
    "npm": "^9.3.1"
  }
}